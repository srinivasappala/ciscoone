export JAVA_OPTS_EXT="${JAVA_OPTS_EXT} -Dorg.apache.jasper.runtime.BodyContentImpl.LIMIT_BUFFER=true -Dorg.apache.jasper.runtime.JspFactoryImpl.USE_POOL=false -Dorg.apache.tomcat.util.http.ServerCookie.ALLOW_EQUALS_IN_VALUE=true -XX:+DisableExplicitGC -Xss512k -XX:ParallelGCThreads=4 -XX:+HeapDumpOnOutOfMemoryError  -XX:HeapDumpPath=${OPENSHIT_LOG_DIR} -verbose:gc -XX:+PrintGCDateStamps -XX:+PrintGCDetails  -XX:+PrintGCCause -XX:+PrintGCApplicationStoppedTime   -XX:-UseGCLogFileRotation -XX:GCLogFileSize=5M -XX:NumberOfGCLogFiles=4 -Xloggc:${OPENSHIT_LOG_DIR}gc.log -XX:+PrintTenuringDistribution -XX:MaxPermSize=256m"
